{
  "name": "日英分割",
  "version": "1.0.1",
  "minimum_chrome_version": "35.0",
  "manifest_version": 2,
  "description": "日本語と英語の間に半角スペースを挿入します",
  "background": {
    "scripts": [ "background.js" ],
    "persistent": false
  },
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "permissions": [
    "contextMenus"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "homepage_url": "https://github.com/jinjor/ja-en-separator"
}
